#include <iostream>
using namespace std;

int main()
{
  const double RISE_PER_YEAR = 1.5
  int five_years = 5;
      seven_years = 7;
      ten_years = 10;
      float after_5 = RISE_PER_YEAR * five_years;
      float after_7 = RISE_PER_YEAR * seven_years;
      float after_10 = RISE_PER_YEAR * ten_years;
      
      cout << "Ocean's level will grow " << after_5
      cout << millimeters after five years." << endl;
      cout << "Ocean's level will grow " << after_7
      cout << millimeters after seven years." << endl;
      cout << "Ocean's level will grow " << after_10
      cout << millimeters after ten years." << endl;
      return 0;
}
